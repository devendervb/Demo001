<core:FragmentDefinition xmlns:form="sap.ui.layout.form"
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
>
	<Dialog
		id="helloDialog"
		title="Hello "
	>
    <content>
     <form:SimpleForm id="idsf">
      <form:content>
         <Label text="ID"></Label>
         <Input id="formId"/>
         <Label text="Title"></Label>
         <Input id="formTitle"/>
         <Label text="Author"></Label>
         <Input id="formAuthor"/>
         <Label text="Price"></Label>
         <Input id="formPrice"/>
         <Label text="Stock"></Label>
         <Input id="formStock"/>
      </form:content>
     </form:SimpleForm>
     </content>
    <beginButton>
      <Button text="Create Book" press=".onCreateBook"></Button>
    </beginButton>
     <endButton>
      <Button text="Close" press=".onDialogClose"></Button>
    </endButton>
    </Dialog>
</core:FragmentDefinition>
